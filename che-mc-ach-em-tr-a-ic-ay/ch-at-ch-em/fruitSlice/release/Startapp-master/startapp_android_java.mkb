#!/usr/bin/env mkb
# Builder mkb file for the java portion of the startapp extension on android
platform JAVA

files
{
    (source/android)
    startapp.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"
librarypath "$MARMALADE_ROOT/extensions/startapp/StartAppSearchSDK-2.0.1.jar "

option output-name=lib/android/startapp.jar
